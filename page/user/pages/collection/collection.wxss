page {
  height: 100%;
  padding-top:108rpx; 
}
page .collectionBar {
  width: 100%;
  height: 108rpx;
  border-bottom: 1px solid rgba(237,237,237,1);
  position: fixed;
  background: #fff;
  top: 0;
  left: 0;
  z-index: 20;
}
.collectionBar .collectionBar-item {
  display: inline-block;
  width: 33.3%;
  height: 100%;
  text-align: center;
  line-height: 108rpx;
  font-size:38rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  position: relative;
}
.collectionBar .collectionBar-itemActive {
  color:rgba(223,32,32,1);
  font-weight:600;
}
.collectionBar .collectionBar-item .tap-bottom {
  position: absolute;
  bottom: 0;
  left: 50%;
  margin-left: -37rpx;
  width:74rpx;
  height:4rpx;
  background:rgba(223,32,32,1);
  border-radius:2px;
}
page .swiper {
  min-height: 100%;
}
page .swiper swiper-item {
  overflow-y: auto;
}
page .item {
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #fff; }
  page .item:last-of-type .vedio-content {
    border-bottom: none; }
  .vedio-content .item .left {
    position: relative;
    width: 260rpx;
    height: 160rpx; }

page .vedio-content {
  display: flex;
  align-items: center;
  padding: 30rpx 0;
  box-sizing: border-box;
  border-bottom: 1rpx solid #e9e9e9; }
.vedio-content .left {
  position: relative;
}
.vedio-content .left .poster{
  width: 260rpx;
  height: 160rpx !important;
  border-radius: 8rpx; }
page .live-container {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 30rpx;
}
.live-container .live-content .tips {
  position: fixed;
  background: linear-gradient(90deg, #f3711b 0%, #ee8a48 100%);
  color: #fff;
  font-size: 26rpx;
  padding: 5rpx 10rpx;
  border-radius: 8rpx 8rpx 8rpx 0;
}
.live-container .live-content {
  display: flex;
  box-sizing: border-box;
  /* border-bottom: 1rpx solid #e9e9e9; */
  flex-direction: column;
  width: 48%;
  /* padding: 0 10rpx; */
}
.live-container .live-content .top {
  height: 240rpx;
  width: 100%;
}
.live-container .live-content .poster{
  width: 100%;
  height: 100%!important;
  border-radius: 8rpx;
}
.live-container .live-content .bottom {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  padding: 15rpx 0 30rpx 0;
}
page .left .icon {
  position: absolute;
  width: 52rpx;
  height: 52rpx;
  margin: auto;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  opacity: 0.8; }

page .right {
  flex: 1;
  width: 410rpx;
  margin-left: 20rpx; }
  page .right .name {
    line-height: 56rpx;
    font-size: 40rpx;
