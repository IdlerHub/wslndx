/* page/vote/pages/voteIndex/voteIndex.wxss */
page{
  height: 100%;
}
.vote{
  width: 100%;
  height: 1354rpx;
  background: url('https://hwcdn.jinlingkeji.cn/uploads/images/vote-bg.png') no-repeat center;
  background-size: 100% 100%;
}
.vote-img{
  width: 100%;
  height: 1140rpx;
  z-index: -1;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
}
.vote-header{
  /* padding: 0 30rpx; */
  /* padding-right: 30rpx; */
  display: flex;
  justify-content: space-between;
  /* margin-bottom: 627rpx; */
  margin-bottom: 693rpx;
}
.header-left{
  max-width: 604rpx;
  height: 64rpx;
  padding-left: 30rpx;
  display: flex;
  align-items: center;
  margin-right: 30rpx;
  /* padding: 12rpx 0; */
  background: linear-gradient(90deg,rgba(86,110,93,1) 0%,rgba(71,98,89,1) 100%);
  border-radius:0 32rpx 32rpx 0;
}
.header-left image{
  width: 36rpx;
  height: 29rpx;
}
.header-left .text{
  width: 509rpx;
  height:40rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,0.8);
  line-height:40rpx;
  margin-left: 12rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.header-left .text .swiper-item{
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.header-right{
  width: 86rpx;
  height: 91rpx;
  margin-right: 30rpx;
  position: relative;
}
.header-right .rule{
  width: 86rpx;
  height: 91rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.header-right .rule-text{ 
  width:58rpx;
  height:64rpx;
  font-size:28rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight: 500;
  color:#FFCCCC;
  line-height:32rpx;
  position: absolute;
  top: 10rpx;
  left: 14rpx;
  bottom: 17rpx;
}
.vote-search{
  width:690rpx;
  height:88rpx;
  margin: 0 30rpx;
  /* background:#C70001; */
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
}
.vote-search .search-left{
  flex: 1;
  height: 100%;
  background: #AD0702;
  display: flex;
  align-items: center;
}
.vote-search .search-left image{
  width: 39rpx;
  height: 38rpx;
  margin-left: 28rpx;
}
.vote-search .search-left input{
  width: 410rpx;
  height: 100%;
  margin-left: 16rpx;
  color:#FFB5B0;
}
.search-ipt{
  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:#FFB5B0;
  line-height:50rpx;
}
.search-btn{
  width: 148rpx;
  height: 88rpx;
  background:#950500;
  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  color:white;
  text-align: center;
  line-height: 88rpx;
}

.swiper-list{
  height: 80rpx;
  display: flex;
  justify-content: space-start;
  white-space: nowrap;
  overflow-x: auto;
}
.vote-body{
  position: relative;
}
.classify-img{
  width: 90rpx;
  height: 80rpx;
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  /* background: #1050FF; */
  /* box-shadow:0rpx 0rpx 9rpx 0rpx rgba(29,0,151,1); */
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 99;
}
.classify-img image{
  width: 100%;
  height: 100%;
}
.scroll{
  margin-right: 90rpx;
}
.swiper-list .swiper-item{
  min-height: 80rpx;
  padding: 0 38rpx;
  line-height: 80rpx;
  text-align: center;

  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(247,247,247,.8);
}

