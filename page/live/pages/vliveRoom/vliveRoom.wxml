<!--page/live/pages/liveVideo/liveVideo.wxml-->
<live-player src="{{liveDetail.liveUrl}}" class="player" autoplay>
  <image
    src='{{liveDetail.cover || "https://ss2.bdstatic.com/70cFvnSh_Q1YnxGkpoWK1HF6hhy/it/u=2170124706,2579447508&fm=26&gp=0.jpg"}}'
    class="testBag"></image>
  <liveTop liveDetail="{{liveDetail}}" bind:checkFollow="checkFollow"/>
  <talkCommon talkList="{{talkList}}" wx:if="{{!close}}" />
  <navigator class="downloadeApp"
    url="/pages/education/education?type=0&url=https://mp.weixin.qq.com/s/vSd8XBQDQkvqVX_kt_YyTQ" hover-class="none">
    <image src="../../images/dowloafeApp.png" bindtap="downloadeApp"></image>
  </navigator>
  <liveBottom bind:sendMsg="sendMsg" bind:praise="praise"/>
</live-player>