/* page/live/pages/liveVideo/liveVideo.wxss */
page {
  width: 100vw;
  height: 100vh;
  background-color: #161925;
}

.backgroundlive {
  width: 100vw;
  height: 100vh;
  position: fixed;
  opacity: 0;
  top: 0;
}

.v-player {
  width: 100vw;
  height: 100vh;
}

.h-player {
  width: 100vw;
  height: 422rpx;
  position: fixed;
  top: 220rpx;
  z-index: 999999999;
}


.h-player image {
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  right: 0;
  bottom: 13rpx;
  padding: 30rpx;
  z-index: 99999;
}

.playCover image {
  width: 100%;
  height: 100%;
  padding: 0;
}

.h-player .liveRemind {
  /* width: 414rpx; */
  background: linear-gradient(360deg, #FF7E56 0%, #F2323A 100%);
  position: absolute;
  bottom: -108rpx;
  left: 20rpx;
  border-radius: 0 100rpx 100rpx 30rpx;
  display: flex;
  align-items: flex-start;
  padding: 20rpx 40rpx 20rpx 20rpx;
  z-index: 99999999991;
}

.h-player .liveRemind image {
  width: 34rpx;
  height: 38rpx;
  margin-right: 20rpx;
  position: unset;
}

.h-player .liveRemind .remindTxt {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  margin-top: -8rpx;
}

.h-player .liveRemind .remindTxt text {
  font-size: 28rpx;
  font-weight: 500;
  color: #FFFFFF;
}

.h-player .liveRemind .remindTxt .text {
  font-size: 28rpx;
  font-weight: 500;
  color: #FFFFFF;
}

.h-player .liveRemind .remindTxt .liveName {
  max-width: 300rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.h-player .liveRemind .remindTxt text .time {
  display: inline-block;
  padding:0rpx 6rpx;
  text-align: center;
  /* line-height: 40rpx; */
  font-size: 28rpx;
  font-weight: 500;
  color: #F2323A;
  background: #fff;
  border-radius: 4rpx;
  margin-top: 11rpx;
}

.testBag {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
}

.downloadeApp {
  width: calc(102vmin / 7.5);
  height: calc(105vmin / 7.5);
  position: fixed;
  bottom: calc(199vmin / 7.5);
  right: calc(10vmin / 7.5);
  z-index: 9999999999999999999;
  display: flex;
  align-items: center;
  justify-content: center;
}

.joinclass {
  width: calc(94vmin / 7.5);
  height: calc(100vmin / 7.5);
  position: fixed;
  bottom: calc(199vmin / 7.5);
  right: calc(10vmin / 7.5);
  z-index: 9999999999999999999;
  display: flex;
  align-items: center;
  justify-content: center;
}

/* 下载app */
.joinclass image {
  width: 100%;
  height: 100%;
}

.downloadeApp image {
  width: 100%;
  height: 100%;
}

@keyframes showImg {
  from {
    transform: rotateY(-90deg)
  }
  to {
    transform: rotateY(0deg)
  }
}

@keyframes loseImg {
  from {
    transform: rotateY(0deg)
  }
  to {
    transform: rotateY(-90deg)
  }
}



.statusBox {
  width: 100vw;
  position: fixed;
  top: 400rpx;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  z-index: 99999;
}

.statusBox text:nth-child(1) {
  font-size: 36rpx;
  font-weight: 400;
  color: #FFFFFF;
}

.statusBox text:nth-child(2) {
  font-size: 28rpx;
  font-weight: 400;
  color: #FFFFFF;
  margin: 10rpx 0 40rpx 0;
}

.statusBox button {
  width: 200rpx;
  height: 80rpx;
}

.statusBox button image {
  width: 100%;
  height: 100%;
}

.teacherBoxmask {
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0;
  z-index: 9999999999;
}

.teacherBox {
  width: 749rpx;
  height: 446rpx;
  background: #FFFFFF;
  border-radius: 30rpx 30rpx 0rpx 0rpx;
  position: fixed;
  bottom: 0;
  z-index: 99999999999;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 30rpx;
  box-sizing: border-box;
  animation: 0.5s show;
}


.teacherMove {
  bottom: -500rpx;
  animation: 0.5s showMove;
}

.teacherBox .icondel {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  padding: 32rpx;
  top: -2rpx;
  right: -2rpx;
}

.teacherBox .avatar {
  width: 100rpx;
  height: 100rpx;
  border: 1rpx solid #EEE;
  border-radius: 50%;
}

.teacherBox text:nth-child(3) {
  font-size: 40rpx;
  font-weight: 500;
  color: #333333;
  margin: 16rpx 0;
  line-height: 40rpx;
}

.teacherBox text:nth-child(4) {
  font-size: 36rpx;
  font-weight: 400;
  color: #666666;
  line-height: 36rpx;
}

.teacherBox .addBtn {
  width: 480rpx;
  height: 88rpx;
  background: #F2323A;
  border-radius: 55rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 42rpx;
  font-weight: 500;
  color: #FFFFFF;
  margin-top: 30rpx;
}

@keyframes show {
  from {
    bottom: -500rpx;
  }

  to {
    bottom: 0;
  }
}

@keyframes showMove {
  from {
    bottom: 0;
  }

  to {
    bottom: -500rpx;
  }
}


.textarea {
  width: 100vw;
  height: 120rpx;
  background: #ededed;
  padding: 30rpx;
  box-sizing: border-box;
  position: fixed;
  bottom: -120rpx;
}

.liveFloat {
  width: calc(90vmin / 7.5);
  height: calc(102vmin / 7.5);
  position: absolute;
  right: calc(16vmin / 7.5);
  bottom: calc(364vmin / 7.5);
  z-index: 99999999993;
}