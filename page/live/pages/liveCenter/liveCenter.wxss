/* page/live/pages/liveCenter/liveCenter.wxss */
.center-list {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}
.center-list .center-item{
    width: 250rpx;
    height: 285rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    border-bottom: 2rpx solid #EDEDED;
    border-right: 2rpx solid #EDEDED;
}
.center-list .center-item:nth-child(3n) {
    border-right: none;
}
.center-list .center-item .item-img{
    width: 130rpx;
    height: 130rpx;
    margin-bottom: 9rpx;
    border-radius: 20rpx;
    overflow: hidden;
    border: 1px solid #ededed;
}
.center-list .center-item .item-text{
    font-size: 32rpx;
    font-family: PingFang-SC-Medium, PingFang-SC;
    font-weight: 500;
    color: #333333;
}