/* page/live/pages/hliveRoom/hliveRoom.wxss */
page {
  width: 100vw;
  height: 100vh;
  background-color: #000;
}


.h-player {
  width: 100vw;
  height: 100vh;
}

.testBag {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
}

.downloadeApp {
  width: calc(102vmin / 7.5);
  height: calc(100.81vmin / 7.5);
  position: fixed;
  bottom: calc(199vmin / 7.5);
  right: calc(20vmin / 7.5);
  z-index: 9999999999999999999;
}

/* 下载app */
.downloadeApp image {
  width: 100%;
  height: 100%;
}

.statusBox {
  width: 100vw;
  position: fixed;
  top: calc(100vmin / 7.5);
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  z-index: 99999;
}

.statusBox text:nth-child(1) {
  font-size: calc(36vmin / 7.5);
  font-weight: 400;
  color: #FFFFFF;
}

.statusBox text:nth-child(2) {
  font-size: calc(28vmin / 7.5);
  font-weight: 400;
  color: #FFFFFF;
  margin: 10rpx 0 40rpx 0;
}

.statusBox button {
  width: calc(200vmin / 7.5);
  height: calc(80vmin / 7.5);
}

.statusBox button image{
  width: 100%;
  height: 100%;
}