
.centerBox {
  margin: 0rpx 30rpx 0rpx 30rpx;
  background:rgba(255,255,255,1);
  /* border-radius: 0 0 10rpx 10rpx; */
  border-radius:10rpx;
  margin-bottom: 20rpx;
}

.centerBox .title {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:36rpx;
  font-weight:400;
  color: rgba(153,153,153,1);
  position: relative;
  z-index: 5;
}
.centerBox .content {
  padding: 0 30rpx;
  background:rgba(255,255,255,1);
} 
.centerBox .contentTop {
  height: 108rpx;
  border-radius:10rpx 10rpx 0 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  position: relative;
  background:rgba(255,255,255,1);
}
.centerBox .contentTop text {
  font-size:36rpx;
  font-weight:600;
  color:rgba(51,51,51,1);
  font-family:PingFangSC-Medium,PingFang SC;
}
.centerBox .contentTop text:nth-child(2) {
  margin-left: 48rpx;
}
.centerBox .contentTop .heng {
  height: 2rpx;
  background: rgba(237,237,237,1);
  position: absolute;
  bottom: 0;
  right: 30rpx;
  left: 30rpx;
}
.centerBox .content .list {
  height: calc(100vh - 335rpx);
  /* height: calc(100vh - 330rpx); */
}

.contentList {
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
  border-bottom: 1rpx solid rgba(237,237,237,1);
}
.contentList:last-child {
  border: 0;
}
 .contentList .number {
   font-size: 36rpx;
  width: 60rpx;
  text-align: center;
}
.contentList .name {
  text-align: center;
  flex: 1;
  width: 0;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  padding-left: 80rpx;
  font-size:36rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}
 .contentList .time {
  width: 162rpx;
  text-align: center;
  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-left: 8rpx;
}
.centerBox .content .contentList .img {
  width: 61rpx;
  height: 51rpx;
}