/* 环境变量 */
page {
  background: #fff;
  box-sizing: border-box; }

.popup {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  z-index: 999; }

.moveIn {
  animation: moveIn 0.5s ease-out 0s both; }

.moveOut {
  pointer-events: none;
  animation: moveOut 0.5s ease-out 0s both; }

@keyframes moveIn {
  from {
    opacity: 0; }
  to {
    transform: none;
    opacity: 1; } }

@keyframes moveOut {
  from {
    transform: scale(1);
    opacity: 1; }
  to {
    opacity: 0; } }

@keyframes ani {
  0% {
    background-position: 0%; }
  100% {
    background-position: 100%; } }

 @keyframes flicker {
    0% {
        background-position: 0%;
    }
    50% {
        background-position: 100%;
    }
    100% {
        background-position: 100%;
    }}
button::after {
  border: none; }

button {
  outline: none;
  background-color: none;
  border: none;
  border-radius: inherit;
  padding: 0;
  margin: 0;
  background: none; }

button.button-hover {
  background-color: transparent;
  opacity: 1; }

.line {
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; }

.btn-hover {
  background: none; }

/* 下拉刷新 */
.weui-loadmore {
  width: 100%;
  height: 50px;
  font-size: 30rpx;
  text-align: center;
  margin: 0rpx auto;
  padding-top: 20rpx;
  padding-bottom: 15rpx;
  background: #eee; }

.weui-loadmore.indexdrop {
  padding-top: 60rpx; }

.weui-loading {
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 0rpx auto;
  background-image: url("https://jinling-xcx-dev.obs.cn-north-1.myhuaweicloud.com/images/pro/refresh.png");
  background-size: cover;
  animation: ani 1s steps(23, end) infinite; }

/* 发布动画 */
.publishAni {
  position: absolute;
  top: 50%;
  bottom: 0;
  left: 0;
  right: 0;
  margin: -500rpx auto 0;
  background-image: url("https://jinling-xcx-dev.obs.cn-north-1.myhuaweicloud.com/images/pro/fabu.png");
  background-size: cover;
  width: 460rpx;
  height: 650rpx;
  animation: ani 2s steps(20, end); }

/* 自定义 ActionSheet*/
.ActionSheet {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,0.6);
  z-index: 20;
  animation: bg 1s;
}
  .ActionSheet .ActionSheet-box {
    width:100%;
    height:267rpx;
    background:#fff;
    border-radius:40px 40px 0px 0px;
    position: absolute;
    bottom: 0;
    animation: runTop 0.5s;
  }
  .ActionSheet .ActionSheet-box .ActionSheet-item {
    width:100%;
    height: 118rpx;
    text-align: center;
    line-height: 118rpx;
    font-size:38rpx;
    font-weight:400;
    color:rgba(51,51,51,1);
    border-bottom: 1px solid rgba(237,237,237,1);
  }
.ActionSheet .ActionSheet-box .ActionSheet-item:nth-child(2) { 
  border: 0;
}
.ActionSheet .ActionSheet-cancelBox {
  position: absolute;
  bottom: 0;
  width:750rpx;
  height:148rpx;
  background:rgba(252,252,252,1);
  font-size:38rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
  text-align: center;
  padding-top: 34rpx;
  box-sizing: border-box;
  animation: runTopTow 0.5s;
}
@keyframes runTop {
  0% {
    bottom:-267rpx; }
  100% {
    bottom: 0; } }
@keyframes runTopTow {
  0% {
    bottom:-148rpx; }
  100% {
    bottom: 0; } }
@keyframes bg {
  0% {
    background-color:rgba(0,0,0,0); }
  100% {
    background-color: rgba(0,0,0,0.6); } }