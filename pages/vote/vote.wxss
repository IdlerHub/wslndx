/* pages/vote/vote.wxss */
page{
    height: 100%;
}
.vote{
    min-height: 100vh;
}
.vote-img{
    width: 100%;
    height: 1140rpx;
    z-index: -1;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
}
.vote-header{
    /* padding: 0 30rpx; */
    padding-right: 30rpx;
    display: flex;
    justify-content: space-between;
    margin-bottom: 627rpx;
}
.header-left{
    max-width: 557rpx;
    height: 64rpx;
    padding-left: 30rpx;
    display: flex;
    align-items: center;
    margin-right: 47rpx;
    /* padding: 12rpx 0; */
    background:linear-gradient(270deg,rgba(27,22,236,1) 0%,rgba(14,0,211,1) 100%);
    border-radius:0px 32px 32px 0px;
}
.header-left image{
    width: 36rpx;
    height: 29rpx;
}
.header-left .text{
    width: 509rpx;
    height:40rpx;
    font-size:28rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,0.8);
    line-height:40rpx;
    margin-left: 12rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.header-left .text .swiper-item{
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.header-right{
    width: 86rpx;
    height: 96rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
    display: flex;
    align-items: center;
    flex-flow: column;
    justify-content: center;
    text-align: center;
}
.header-right>view{
    width: 58rpx;
    height: 64rpx;
    line-height: 32rpx;
    font-size:28rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(16,89,245,1);
    text-align: center;
}

.vote-search{
    width:690rpx;
    height:80rpx;
    margin: 0 30rpx;
    
    margin-bottom: 32rpx;
    background:rgba(13,0,198,1);
    border-radius:40rpx;
    display: flex;
    align-items: center;
}
.vote-search image{
    width: 39rpx;
    height: 38rpx;
    margin-left: 32rpx;
}
.vote-search input{
    width: 410rpx;
    margin-left: 22rpx;
    color:rgba(255,255,255,0.4);
}
.search-ipt{
    font-size:36rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,0.4);
    line-height:50rpx;
}
.search-btn{
    width:148rpx;
    height:80rpx;
    margin-left: 39rpx;
    background:rgba(0,0,0,0.2);
    border-radius:0rpx 100rpx 100rpx 0rpx;

    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    font-size:36rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,1);
}

.swiper-list{
    height: 80rpx;
    display: flex;
    justify-content: space-start;
    white-space: nowrap;
    overflow-x: auto;
}
.vote-body{
    position: relative;
}
.classify-img{
    width: 90rpx;
    height: 80rpx;
    position: absolute;
    right: 0;
    top: 0;
    bottom: 0;
    /* background: #1050FF; */
    /* box-shadow:0rpx 0rpx 9rpx 0rpx rgba(29,0,151,1); */
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 99;
}
.classify-img image{
    width: 100%;
    height: 100%;
}
.scroll{
    margin-right: 90rpx;
}
.swiper-list .swiper-item{
    min-height: 80rpx;
    padding: 0 38rpx;
    line-height: 80rpx;
    text-align: center;

    font-size:36rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(247,247,247,.8);
}

.swiper-list .active{
    font-weight: 600;
    background:#00A1FF;
    border-radius:20rpx 20rpx 0rpx 0rpx;
    color:rgba(247,247,247,1);
}
