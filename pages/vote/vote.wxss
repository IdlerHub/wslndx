/* pages/vote/vote.wxss */
page{
    height: 100%;
}
.vote{
    min-height: 100%;
}
.vote-img{
    width: 100%;
    height: 1448rpx;
    z-index: -1;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
}
.vote-header{
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 820rpx;
}
.header-left{
    max-width: 557rpx;
    display: flex;
    align-items: center;
    margin-right: 47rpx;
    padding: 12rpx 0;
}
.header-left image{
    width: 36rpx;
    height: 29rpx;
}
.header-left .text{
    max-width: 509rpx;
    height:40rpx;
    font-size:28rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,0.8);
    line-height:40rpx;
    margin-left: 12rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.header-right{
    width: 86rpx;
    height: 96rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
    display: flex;
    align-items: center;
    flex-flow: column;
    justify-content: center;
}
.header-right text{
    font-size:28rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(16,89,245,1);
}

.vote-search{
    width:690rpx;
    height:80rpx;
    margin: 0 30rpx;
    
    margin-bottom: 32rpx;
    background:rgba(13,0,198,1);
    border-radius:40rpx;
    display: flex;
    align-items: center;
}
.vote-search image{
    width: 39rpx;
    height: 38rpx;
    margin-left: 32rpx;
}
.vote-search input{
    width: 410rpx;
    margin-left: 22rpx;
    color:rgba(255,255,255,0.4);
}
.search-ipt{
    font-size:36rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,0.4);
    line-height:50rpx;
}
.search-btn{
    width:148rpx;
    height:80rpx;
    margin-left: 39rpx;
    background:rgba(0,0,0,0.2);
    border-radius:0rpx 100rpx 100rpx 0rpx;

    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    font-size:36rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,1);
}

.swiper-list{
    height: 80rpx;
    display: flex;
    justify-content: flex-start;
    white-space: nowrap;
    overflow-x: auto;
}
.vote-body{
    position: relative;
}
.classify-img{
    width: 90rpx;
    height: 80rpx;
    position: absolute;
    right: 0;
    top: -1rpx;
    bottom: 0;
    background: #1050FF;
    box-shadow:0rpx 0rpx 9rpx 0rpx rgba(29,0,151,1);
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 99;
}
.classify-img image{
    width: 39rpx;
    height: 39rpx;
}
.scroll{
    margin-right: 90rpx;
}
.swiper-list .swiper-item{
    min-height: 80rpx;
    padding: 0 38rpx;
    line-height: 80rpx;
    text-align: center;

    font-size:36rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:#F7F7F7;
}

.active{
    background:rgba(25,117,255,1);
    border-radius:20rpx 20rpx 0rpx 0rpx;
}
.vote-list{
    width:100%;
    padding:30rpx;
    display: flex;
    flex-flow: wrap;
    align-items: center;
    background:rgba(25,117,255,1);
    padding-bottom: 128rpx;
}
.vote-item{
    width: 330rpx;
    height: 366rpx;
    margin-right: 30rpx;
    margin-bottom: 30rpx;
    overflow: hidden;
    border-radius:10rpx;
}

.item-up{
    height: 220rpx;
    background: blue;
    position: relative;
    overflow: hidden;
}
.item-img,
.item-up .item-img image{
    width: 100%;
    height: 100%;
}
.item-rank{
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    text-align: center;
    min-width: 78rpx;
    padding: 11rpx;
    display: flex;
    align-items: center;
    background:linear-gradient(90deg,rgba(255,75,0,1) 0%,rgba(255,136,0,1) 100%);
    border-radius:10rpx 100rpx 100rpx 0px;

    font-size:34rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(255,255,255,1);
}
.item-rank .rank-text{
    width:45rpx;
    height: 40rpx;
    font-size:18rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,1);
    display: flex;
    align-items: center;
    flex-flow: column;
    justify-content: center;
    margin-right: 12rpx;
}
.item-rank .num{
    font-size:44rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,1);
    margin-right: 12rpx;
}
.item-classify{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    bottom: 10rpx;
    right: 10rpx;
}
.item-down{
    height: 146rpx;
    background: white;
}
.item-name{
    padding-top: 12rpx;
    margin-left: 20rpx;
    margin-bottom: 8rpx;
    font-size:36rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(51,51,51,1);
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.item-footer{
    margin: 20rpx;
    margin-top: 0rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.item-zan{
    font-size:30rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(102,102,102,1);
}
.zan-btn{
    width:154rpx;
    height:56rpx;
    line-height: 56rpx;
    background:rgba(223,32,32,1);
    border-radius:28rpx;

    text-align: center;
    font-size:30rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,1);
}
.vote-footer{
    position: fixed;
    bottom: 40rpx;
    right: 30rpx;
    left: 30rpx;
    display: flex;
    justify-content: center;
    z-index: 100;
}
.vote-footer>image{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: -1;
}
.footer-left,
.footer-right{
    display: flex;
    justify-content: center;
    align-items: center;
    
    height: 98rpx;
    line-height: 98rpx;
    border-radius:52rpx;

    font-size:40rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(255,255,255,1);
}
.footer-left{
    width: 400rpx;
    box-shadow:3rpx 3rpx 12rpx 0rpx rgba(255,30,0,0.7);
    border-radius:49rpx 0rpx 0rpx 49rpx;
}
.footer-right{
    width: 340rpx;
    border-radius:0rpx 49rpx 49rpx 0rpx;
    box-shadow:3rpx 3rpx 12rpx 0rpx rgba(255,30,0,0.7);
}
.footer-left image{
    width: 44rpx;
    height: 55rpx;
    margin-right: 16rpx;
}
.footer-right image{
    width: 50rpx;
    height: 50rpx;
    margin-right: 16rpx;
}