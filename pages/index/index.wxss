/* 骨架屏 */

.skeletonaAtive {
  background: #EDEDED;
}

.skeletonSwiper {
  height: 240rpx;
  position: relative;
  z-index: 2;
}

.skeletonSwiper .skeletonaAtive {
  width: 100%;
  height: 100%;
}

.skeleton {
  width: 100%;
  padding: 20rpx;
  background: white;
  overflow: hidden;
  box-sizing: border-box;
  animation: skeleton 1.5s infinite;
}

.skeleton .skeletonImg {
  width: 260rpx;
  height: 160rpx;
  background: #EDEDED;
  float: left;
  border-radius: 10rpx;
}

.skeleton .skeletonTitle {
  width: 420rpx;
  height: 32rpx;
  background: #EDEDED;
  border-radius: 3rpx;
  float: right;
  margin-top: 14rpx;
}

.skeleton .skeletonDaram {
  width: 123rpx;
  height: 32rpx;
  background: #EDEDED;
  border-radius: 3rpx;
  float: left;
  margin-left: 30rpx;
}

.skeleton .skeletonContent {
  width: 420rpx;
  height: 32rpx;
  background: #EDEDED;
  border-radius: 3rpx;
  float: right;
  margin: 15rpx 0 15rpx 0;
}

@keyframes skeleton {
  0% {
    opacity: 1;
  }

  50% {
    opacity: 0.5;
  }

  100% {
    opacity: 1;
  }
}

