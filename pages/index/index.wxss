.now {
  position: fixed;
  bottom: 0;
  width: 690rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  color: #ed5708;
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #fff;
  height:100rpx;
  box-shadow:0px 2px 10px 0px rgba(185,184,184,0.5);
  left: 50%;
  margin-left: -345rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.now image:nth-child(1) {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.now image:nth-child(3) {
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
}
.now  .line {
  min-width: 510rpx;
  height: 100%;
}
@keyframes nowshow {
  0% {
    bottom: -88rpx;
  }
  100% {
    bottom: 138rpx;
  }
}
