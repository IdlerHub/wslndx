.fixed {
  position: fixed;
  z-index: 999;
}

.now {
  position: relative;
  margin: 20rpx auto 0;
  width: 690rpx;
  line-height: 95rpx;
  font-size: 36rpx;
  color: #ed5708;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.now .line {
  position: relative;
  z-index: 1;
