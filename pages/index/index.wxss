@charset "UTF-8";
@import "../../template/template.wxss";

page {
  background: #EDEDED;
}

/* 环境变量 */
.container {
  height: 100vh;
  position: relative;
  background: rgba(248, 248, 248, 1);
}

.top {
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  background: rgba(248, 248, 248, 1);
  position: relative;
  z-index: 2;
}

.topBg {
  width: 100vw;
  height: 320rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}

.top .avatarBox {
  width: 64rpx;
  height: 64rpx;
  border: 1rpx solid #EDEDED;
  border-radius: 50%;
  overflow: hidden;
}

.top .img {
  width: 100%;
  height: 100%;
}

.top .userNone {
  width: 64rpx;
  height: 64rpx;
}

.top text {
  display: block;
  width: 400rpx;
  margin-left: 20rpx;
  font-size: 36rpx;
  color: #333333;
  font-weight: 500;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  height: 56rpx;
}

.top .loginTxt {
  font-size: 40rpx;
  font-weight: 500;
  color: #333333;
}

.left .left-btn {
  position: absolute;
  z-index: 5;
  border: none;
  opacity: 0;
}

.centerBox {
  padding: 32rpx 0 30rpx 0;
  background: linear-gradient(180deg, #FFFFFF 0%, #F7F7F7 100%);
}

.centerBox .centerImg {
  display: block;
  width: 710rpx;
  height: 170rpx;
  margin: 40rpx auto 0 auto;
}

.centerIcon {
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
}

.centerIcon .centerIconItem {
  width: 25%;
  margin-bottom: 40rpx;
}

.centerIcon .iconBox {
  display: flex;
  flex-direction: column;
  align-items: center;
  line-height: normal;
  position: relative;
}

.centerIcon .iconBox image {
  width: 92rpx;
  height: 92rpx;
}

.centerIcon .iconBox .hotIcon {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 20rpx;
  top: -32rpx;
}

.centerIcon .iconBox .iconCanvas {
  width: 65rpx;
  height: 50rpx;
  position: absolute;
  top: -20rpx;
  right: 12rpx;
}

.centerIcon .iconBox text {
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 0.89);
  margin-top: 9rpx;
}

.navintegral {
  width: 128rpx;
  box-sizing: border-box;
}


.wrap {
  padding-bottom: 278rpx;
  box-sizing: border-box;
}

.swiperBox {
  position: relative;
  /* padding-top: 145rpx; */
}

.swiper {
  height: 270rpx;
  overflow: hidden;
  background: #fff;
  background: #fff;
  position: relative;
  z-index: 2;
}

.swiperBox .indicatorBox {
  position: absolute;
  left: 50%;
  bottom: 20rpx;
  transform: translateX(-50%);
  z-index: 6666;
  display: flex;
  align-items: center;
}

.swiperBox .indicatorBox .indicatorItem {
  width: 26rpx;
  height: 4rpx;
  background: rgba(248, 248, 248, 0.6);
  margin-right: 10rpx;
}

.swiperBox .indicatorBox .indicatorActive {
  background: #F73943;
}

.swiper .slide-image {
  width: 100vw;
  height: 270rpx;
}

.category {
  overflow: hidden;
  background: #f8f8f8;
}

.category .kind {
  background: white;
  margin: 30rpx;
  border-radius: 6rpx;
}

.category .kind .title {
  display: block;
  padding-left: 30rpx;
  height: 96rpx;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #7e7e7e;
  letter-spacing: 0;
  line-height: 96rpx;
}

.category .kind .list {
  display: flex;
  flex-wrap: wrap;
}

.category .kind .list .item {
  width: 30%;
  padding-bottom: 40rpx;
  text-align: center;
  border: solid #e9e9e9;
  border-width: 1rpx 1rpx 0 0;
  flex-grow: 1;
}

.category .kind .list .item:nth-child(3n) {
  border-right-width: 0;
}

.category .kind .list .item .img {
  display: block;
  width: 75rpx;
  height: 66rpx;
  margin: 40rpx auto 30rpx;
}

.category .kind .list .item .name {
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #3a3a3a;
  letter-spacing: 0;
}

.now {
  position: fixed;
  bottom: 0;
  z-index: 2;
  width: 690rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  color: #333;
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #fff;
  height: 100rpx;
  box-shadow: 0px 2px 10px 0px rgba(185, 184, 184, 0.5);
  left: 50%;
  margin-left: -345rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.now .iconImg {
  margin-right: 20rpx;
}

.now image:nth-child(3) {
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
}

.now .line {
  height: 100%;
  flex: 1;
}

.reserve {
  height: 118rpx;
  box-shadow: 0px 5px 16px 0px rgba(0, 0, 0, 0.2);
  border-radius: 16rpx;
}

.reserve .line {
  line-height: 118rpx;
  margin-left: 16rpx;
}

.reserve image {
  width: 64rpx;
  height: 64rpx;
}

.reserve text:nth-child(3) {
  width: 148rpx;
  height: 56rpx;
  background: linear-gradient(90deg, #FF794D 0%, #F63C09 100%);
  border-radius: 8rpx;
  text-align: center;
  line-height: 56rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: #FFFFFF;
}

@keyframes nowshow {
  0% {
    bottom: -88rpx;
  }

  100% {
    bottom: 138rpx;
  }
}

@keyframes nowclose {
  0% {
    bottom: 138rpx;
  }

  100% {
    bottom: -88rpx;
  }
}

/*授权弹框*/

#getinfo-box {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  z-index: 2;
  display: flex;
  align-items: center;
  justify-content: center;
}

#getinfo-box .mask {
  position: absolute;
  z-index: 1;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.3);
}

#getinfo-box .content-box {
  position: absolute;
  z-index: 3;
  width: 604rpx;
  background: #fff;
  border-radius: 20rpx;
}

#getinfo-box .head,
#getinfo-box .footer {
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

#getinfo-box .head text {
  font-size: 40rpx;
  color: #3a3a3a;
}

#getinfo-box .content {
  display: flex;
  flex-direction: column;
  align-items: center;
  border-top: 1rpx solid #e9e9e9;
  border-bottom: 1rpx solid #e9e9e9;
  padding: 40rpx;
}

#getinfo-box .content image {
  width: 74rpx;
  height: 74rpx;
  border-radius: 50%;
}

#getinfo-box .content .lg-tit {
  display: inline-block;
  margin: 30rpx 0;
  font-size: 32rpx;
  border-bottom: 1rpx solid #e9e9e9;
  padding-bottom: 24rpx;
}

#getinfo-box .content .sm-tit {
  font-size: 28rpx;
  color: #7e7e7e;
}

#getinfo-box .footer {
  position: relative;
}

#getinfo-box button {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  color: #3a3a3a;
  height: 100rpx;
}

.btn-comfirm::before {
  content: " ";
  position: absolute;
  height: 100rpx;
  width: 1rpx;
  left: 0;
  bottom: 0;
  background-color: #e9e9e9;
}

#getinfo-box .btn-comfirm {
  color: #df2020;
}

.integration {
  position: fixed;
  z-index: 999999;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.3);
}

.integration .dialog {
  position: absolute;
  width: 568rpx;
  height: 680rpx;
  border-radius: 10rpx;
  left: calc((100% - 556rpx) / 2);
  top: calc((100% - 742rpx) / 2);
  text-align: center;
}

.integration .dialog .bg {
  width: 100%;
  height: 100%;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.integration .dialog .ani {
  position: absolute;
  top: 330rpx;
  width: 539rpx;
  height: 393rpx;
  left: calc((100% - 539rpx) / 2);
  background-image: url("https://hwcdn.jinlingkeji.cn/images/dev/scoreAnimation.png");
  background-size: cover;
  animation: ani 1.5s steps(23, end) infinite;
}

.integration .dialog button {
  width: 366rpx;
  height: 110rpx;
  background: #df2020;
  border-radius: 110rpx;
  margin: 58rpx auto;
  font-family: PingFangSC-Regular;
  font-size: 44rpx;
  line-height: 110rpx;
  color: #fff;
  letter-spacing: 1.83px;
  text-align: center;
}

.integration .dialog icon {
  margin-top: 48rpx;
}

.integration .dialog .share-del {
  width: 84rpx;
  height: 84rpx;
  position: absolute;
  right: 43%;
  bottom: -95rpx;
  z-index: 9999;
}

.outer-link {
  height: 250rpx;
  margin: 30rpx 0 0;
  padding: 40rpx 30rpx;
  background: #efefef;
  display: flex;
  align-items: stretch;
  text-align: left;
  /* display: none; */
  overflow: visible;
}

.outer-link>image {
  width: 270rpx;
  height: 170rpx;
}

.outer-link .dsc {
  position: relative;
  flex: 1;
  padding-left: 20rpx;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}

.outer-link .dsc>text:nth-child(1) {
  font-size: 40rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #3a3a3a;
  line-height: 56rpx;
}

.outer-link .dsc>text:nth-child(2) {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #7e7e7e;
  line-height: 40rpx;
}

.outer-link .dsc image {
  width: 200rpx;
  height: 54rpx;
  position: absolute;
  bottom: 0;
}

