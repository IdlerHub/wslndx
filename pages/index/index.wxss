  font-family: PingFangSC-Regular;
  font-size: 44rpx;
  line-height: 110rpx;
  color: #fff;
  letter-spacing: 1.83px;
  text-align: center;
}

.integration .dialog icon {
  margin-top: 48rpx;
}

