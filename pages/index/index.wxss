}

.tab-item {
  background: #EDEDED;
}

.wrap {
  padding-bottom: 180rpx;
  box-sizing: border-box;
}

.swiperBox {
  position: relative;
}

.swiper {
  height: 240rpx;
  overflow: hidden;
  background: #fff;
  background: #fff;
}

.swiperBox .indicatorBox {
  position: absolute;
  left: 50%;
  bottom: 20rpx;
  transform: translateX(-50%);
  z-index: 6666;
  display: flex;
  align-items: center;
}

.swiperBox .indicatorBox .indicatorItem {
  width: 26rpx;
  height: 4rpx;
