}

.category {
  overflow: hidden;
  background: #f8f8f8;
}

.category .kind {
  background: white;
  margin: 30rpx;
  border-radius: 6rpx;
}

.category .kind .title {
  display: block;
  padding-left: 30rpx;
  height: 96rpx;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #7e7e7e;
  letter-spacing: 0;
  line-height: 96rpx;
}

.category .kind .list {
  display: flex;
  flex-wrap: wrap;
}

.category .kind .list .item {
  width: 30%;
  padding-bottom: 40rpx;
  text-align: center;
  border: solid #e9e9e9;
  border-width: 1rpx 1rpx 0 0;
  flex-grow: 1;
}

.category .kind .list .item:nth-child(3n) {
  border-right-width: 0;
}

.category .kind .list .item .img {
  display: block;
  width: 75rpx;
  height: 66rpx;
  margin: 40rpx auto 30rpx;
}

.category .kind .list .item .name {
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #3a3a3a;
  letter-spacing: 0;
}


.fixed {
  position: sticky;
  z-index: 999;
}

.now {
  position: fixed;
  bottom: 0;
  width: 690rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  color: #ed5708;
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #fff;
  height: 100rpx;
  box-shadow: 0px 2px 10px 0px rgba(185, 184, 184, 0.5);
  left: 50%;
  margin-left: -345rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.now image:nth-child(1) {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}

.now image:nth-child(3) {
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
}

.now .line {
  min-width: 510rpx;
  height: 100%;
  flex: 1;
}

@keyframes nowshow {
  0% {
    bottom: -88rpx;
  }

  100% {
    bottom: 138rpx;
  }
}

@keyframes nowclose {
  0% {
    bottom: 138rpx;
  }

  100% {
    bottom: -88rpx;
  }
}

