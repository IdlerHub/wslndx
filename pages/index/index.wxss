}

.top .img {
  width: 100%;
  height: 100%;
}

.top text {
  display: block;
  width: 400rpx;
  margin-left: 20rpx;
  font-size: 36rpx;
  color: #333333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  height: 56rpx;
}

.left .left-btn {
  position: absolute;
  z-index: 5;
  border: none;
  opacity: 0;
}

.centerIcon {
  display: flex;
  padding: 32rpx 20rpx;
  align-items: center;
  justify-content: space-around;
  background: linear-gradient(180deg, #FFFFFF 0%, #F7F7F7 100%);
}

.centerIcon .iconBox {
  display: flex;
  flex-direction: column;
  align-items: center;
  line-height: normal;
}

.centerIcon .iconBox image {
  width: 92rpx;
  height: 92rpx;
}

.centerIcon .iconBox text {
