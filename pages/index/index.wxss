/*授权弹框*/

#getinfo-box {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  z-index: 2;
  display: flex;
  align-items: center;
  justify-content: center;
}

#getinfo-box .mask {
  position: absolute;
  z-index: 1;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.3);
}

#getinfo-box .content-box {
  position: absolute;
  z-index: 3;
  width: 604rpx;
  background: #fff;
  border-radius: 20rpx;
}

#getinfo-box .head, #getinfo-box .footer {
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

#getinfo-box .head text {
  font-size: 40rpx;
  color: #3a3a3a;
}

#getinfo-box .content {
  display: flex;
  flex-direction: column;
  align-items: center;
  border-top: 1rpx solid #e9e9e9;
  border-bottom: 1rpx solid #e9e9e9;
  padding: 40rpx;
}

#getinfo-box .content image {
  width: 74rpx;
  height: 74rpx;
  border-radius: 50%;
}

#getinfo-box .content .lg-tit {
  display: inline-block;
  margin: 30rpx 0;
  font-size: 32rpx;
  border-bottom: 1rpx solid #e9e9e9;
  padding-bottom: 24rpx;
}

#getinfo-box .content .sm-tit {
  font-size: 28rpx;
  color: #7e7e7e;
}

#getinfo-box .footer {
  position: relative;
}

#getinfo-box button {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  color: #3a3a3a;
  height: 100rpx;
}

.btn-comfirm::before {
  content: " ";
  position: absolute;
  height: 100rpx;
  width: 1rpx;
  left: 0;
  bottom: 0;
  background-color: #e9e9e9;
}

#getinfo-box .btn-comfirm {
  color: #df2020;
}

.integration {
  position: fixed;
  z-index: 2;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.3);
}

.integration .dialog {
  position: absolute;
  width: 556rpx;
  height: 742rpx;
  border-radius: 10rpx;
  left: calc((100% - 556rpx) / 2);
  top: calc((100% - 860rpx) / 2);
  text-align: center;
}

.integration .dialog .bg {
  width: 100%;
  height: 100%;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.integration .dialog .ani {
  position: absolute;
  top: 330rpx;
  width: 539rpx;
  height: 393rpx;
  left: calc((100% - 539rpx) / 2);
  background-image: url("https://hwcdn.jinlingkeji.cn/images/dev/scoreAnimation.png");
  background-size: cover;
  animation: ani 1.5s steps(23, end) infinite;
}

.integration .dialog button {
  width: 366rpx;
  height: 110rpx;
  background: #df2020;
  border-radius: 110rpx;
  margin: 58rpx auto;
  font-family: PingFangSC-Regular;
  font-size: 44rpx;
  line-height: 110rpx;
  color: #fff;
  letter-spacing: 1.83px;
  text-align: center;
}

.integration .dialog icon {
  margin-top: 48rpx;
}

