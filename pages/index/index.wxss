.floaBar {
  width: 120rpx;
  height: 266rpx;
  background: #FFF8EF;
  box-shadow: 0px 5rpx 16rpx 0px rgba(0, 0, 0, 0.2);
  border-radius: 16rpx;
  border: 1px solid #FFE2C2;
  position: fixed;
  right: 18rpx;
  bottom: 346rpx;
  display: flex;
  flex-direction: column;
  z-index: 5;
}

.floaBar::after {
  content: '';
  width: 100%;
  height: 1px;
  background: #FFE2C2;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
}

.floaBar .bar {
  flex: 1;
  width: 100%;
  height: 50%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.floaBar .bar text {
  font-size: 30rpx;
  font-weight: 400;
  color: #4D4D4D;
  margin-top: 5rpx;
}

.floaBar .bar:nth-child(1) image {
  width: 44.18rpx;
  height: 54rpx;
}

.floaBar .bar:nth-child(2) image {
  width: 54rpx;
  height: 54rpx;
}

