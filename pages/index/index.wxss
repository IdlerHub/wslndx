@charset "UTF-8";

/* 环境变量 */
.container {
  height: 100%vh;
  background: rgba(248, 248, 248, 1);
}
.topOne {
  background: white;
}

.topOne .box .left .img {
  background: #f5f6f8;
  border: 0;
}
.topOne .info .name {
  width:320rpx;
  height:56rpx;
  background: #f5f6f8;
  border-radius: 12rpx;
}
.topOne .info .line {
  margin-top: 10rpx;
}
.topOne .info .city {
  background: #f5f6f8;
  width:85rpx;
  height:40rpx;
}
.topOne .info .school {
  background: #f5f6f8;
  width:205rpx;
  height:40rpx;
}
.top {
  position: relative;
  height: 286rpx;
  overflow: hidden;
}

.top .authorize {
  overflow: initial;
}

.top .bg {
  position: absolute;
  left: -25rpx;
  bottom: 0;
  top: 0;
  margin: auto;
  width: 800rpx;
  height: 328rpx;
  filter: blur(20rpx) brightness(0.65);
  transform: scale(1.2);
}

.top .box {
  position: relative;
  z-index: 1;
}

.top .box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 124rpx 30rpx 0;
  box-sizing: border-box;
}

.box .left {
  flex: 1;
  display: flex;
  align-items: center;
  overflow: hidden;
}

.box .left .img {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border: 2rpx solid #b1adac;
  overflow: hidden;
}

.box .right {
  width: 19rpx;
  height: 34rpx;
}

.left .info {
  flex: 1;
  margin: 0 20rpx;
  overflow: hidden;
}

.info .line {
  text-align: left;
  line-height: 62rpx;
}

.info .name {
  /* width: 440rpx; */
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  line-height: 50rpx;
  color: #fff;
}

.info .city {
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #fff;
  line-height: 40rpx;
  border-radius: 4rpx;
  background: #b1adac;
  padding: 0 15rpx;
}

.info .school {
  margin-left: 15rpx;
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #fff;
  line-height: 40rpx;
  border-radius: 4rpx;
  background: #b1adac;
  padding: 0 15rpx;
}

.jumpout {
  background: #fff;
  margin-bottom: 20rpx;
  padding: 26rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.left .left-btn {
  position: absolute;
  z-index: 5;
  border: none;
  opacity: 0;
}
.jumpout .jumpoutBox {
  position: relative;
  height: 120rpx;
}

.jumpout .img {
  width: 330rpx;
  height: 120rpx;
  background: #f5f6f8;
}

.jumpout text {
  position: absolute;
  font-size: 40rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  top: 32rpx;
  left: 30rpx;
}

.nav {
  padding: 0 30rpx 0 40rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #e9e9e9;
  background: #fff;
  height: 100rpx;
}

.nav .item {
  line-height: 100rpx;
  display: inline-block;
  width: 140rpx;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #7e7e7e;
  letter-spacing: 0;
}

.nav .item.active {
  position: relative;
  color: #df2020;
  font-size: 44rpx;
  font-weight: 800;
}

.nav .item.active::after {
  content: "";
  position: absolute;
  bottom: 2rpx;
  left: 25%;
  width: 50%;
  height: 4rpx;
  background: #df2020;
}

.nav .integral {
  height: 100%;
  float: right;
  padding: 10rpx 0 0 30rpx;
  border-left: 1px solid #e9e9e9;
  box-sizing: border-box;
}

.integral  .integralImg {
  width: 96rpx;
  height: 80rpx;
  background: url('https://jinling-xcx-dev.obs.cn-north-1.myhuaweicloud.com/images/pro/integral.png') no-repeat;
  background-size: cover;
  animation: flicker steps(23, end) infinite 2.5s 1s;
}
.navintegral {
  width: 128rpx;
  box-sizing: border-box;
}
.tab-item {
  background: #fff;
}

.wrap {
  padding-bottom: 180rpx;
  box-sizing: border-box;
}

.recommend .item {
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #fff;
}

.swiper {
  /* width: 100%; */
  height: 270rpx;
  overflow: hidden;
  margin: 30rpx 30rpx 0 30rpx;
  /* box-sizing: border-box; */
  background: #fff;
  border-radius: 8rpx;
}

.swiper swiper-item {
  border-radius: 8rpx;
}

.swiper .slide-image {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}

.skeleton {
  width: 100%;
  padding: 30rpx;
  background: white;
  overflow: hidden;
  box-sizing: border-box;
}

.skeleton .skeletonImg {
  width: 260rpx;
  height: 160rpx;
  background: #f5f6f8;
  float: left;
  border-radius: 10rpx;
}

.skeleton .skeletonTitle {
  width: 400rpx;
  height: 32rpx;
  background: #f5f6f8;
  border-radius: 3rpx;
  float: right;
  margin-top: 14rpx;
}

.skeleton .skeletonDaram {
  width: 123rpx;
  height: 32rpx;
  background: #f5f6f8;
  border-radius: 3rpx;
  float: left;
  margin-left: 30rpx;
}

.skeleton .skeletonContent {
  width: 400rpx;
  height: 32rpx;
  background: #f5f6f8;
  border-radius: 3rpx;
  float: right;
  margin: 15rpx 0 15rpx 0;
}

.item .right .title {
  line-height: 50rpx;
  font-size: 38rpx;
  color: #3a3a3a;
  letter-spacing: 0;
  display: -webkit-box;
  overflow: hidden;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.item .right .createtime {
  display: block;
  margin-top: 14rpx;
}

.recommend .item .content {
  display: flex;
  align-items: center;
  padding: 30rpx 0;
  box-sizing: border-box;
  border-bottom: 1rpx solid #e9e9e9;
}

.recommend .item .content.big {
  flex-direction: column;
  align-items: flex-start;
}

.recommend .item .content.big .left {
  width: 100%;
  height: 364rpx;
}

.recommend .item .content.big .left .poster {
  width: 100%;
  height: 364rpx;
}

.recommend .item .content.big .left .icon {
  width: 100rpx;
  height: 100rpx;
}

.recommend .item .content.big .right {
  width: 100%;
  margin-left: 0;
  margin-top: 20rpx;
}

.recommend .item .content.big .right .actor {
  margin-bottom: 0;
}

.recommend .item .content.big .right .info {
  display: none;
}

.recommend .item:last-of-type .content {
  border-bottom: none;
}

.recommend .item .left {
  position: relative;
  width: 260rpx;
  height: 160rpx;
}

.recommend .left .poster {
  width: 260rpx;
  height: 160rpx;
  border-radius: 8rpx;
}

.recommend .left .icon {
  position: absolute;
  width: 52rpx;
  height: 52rpx;
  margin: auto;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  opacity: 0.7;
}

.recommend .right {
  flex: 1;
  width: 410rpx;
  margin-left: 20rpx;
}

.recommend .right .name {
  line-height: 56rpx;
  font-size: 40rpx;
  color: #3a3a3a;
  letter-spacing: 0;
}

.recommend .right .actor {
  line-height: 40rpx;
  font-size: 28rpx;
  color: #8a8a8a;
  letter-spacing: 0;
  margin: 8rpx 0;
}

.recommend .right .info {
  line-height: 40rpx;
  font-size: 28rpx;
  color: #bfbfbf;
  letter-spacing: 0;
}

.recommend .right .info .key {
  color: #8a8a8a;
  margin-right: 12rpx;
}

.playNum {
  margin-left: 40rpx;
}

.category {
  overflow: hidden;
  background: #f8f8f8;
}

.category .kind {
  background: white;
  margin: 30rpx;
  border-radius: 6rpx;
}

.category .kind .title {
  display: block;
  padding-left: 30rpx;
  height: 96rpx;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #7e7e7e;
  letter-spacing: 0;
  line-height: 96rpx;
}

.category .kind .list {
  display: flex;
  flex-wrap: wrap;
}

.category .kind .list .item {
  width: 30%;
  padding-bottom: 40rpx;
  text-align: center;
  border: solid #e9e9e9;
  border-width: 1rpx 1rpx 0 0;
  flex-grow: 1;
}

.category .kind .list .item:nth-child(3n) {
  border-right-width: 0;
}

.category .kind .list .item .img {
  display: block;
  width: 75rpx;
  height: 66rpx;
  margin: 40rpx auto 30rpx;
}

.category .kind .list .item .name {
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #3a3a3a;
  letter-spacing: 0;
}

