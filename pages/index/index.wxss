#getinfo-box {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  z-index: 2;
  display: flex;
  align-items: center;
  justify-content: center;
}

#getinfo-box .mask {
  position: absolute;
  z-index: 1;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.3);
}

#getinfo-box .content-box {
  position: absolute;
  z-index: 3;
  width: 604rpx;
  background: #fff;
  border-radius: 20rpx;
}

#getinfo-box .head, #getinfo-box .footer {
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

#getinfo-box .head text {
  font-size: 40rpx;
  color: #3a3a3a;
}

#getinfo-box .content {
  display: flex;
  flex-direction: column;
  align-items: center;
  border-top: 1rpx solid #e9e9e9;
  border-bottom: 1rpx solid #e9e9e9;
  padding: 40rpx;
}

#getinfo-box .content image {
  width: 74rpx;
  height: 74rpx;
  border-radius: 50%;
}

#getinfo-box .content .lg-tit {
  display: inline-block;
  margin: 30rpx 0;
  font-size: 32rpx;
  border-bottom: 1rpx solid #e9e9e9;
  padding-bottom: 24rpx;
}

#getinfo-box .content .sm-tit {
  font-size: 28rpx;
  color: #7e7e7e;
}

#getinfo-box .footer {
  position: relative;
}

#getinfo-box button {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  color: #3a3a3a;
  height: 100rpx;
}

.btn-comfirm::before {
  content: " ";
  position: absolute;
  height: 100rpx;
  width: 1rpx;
  left: 0;
  bottom: 0;
