/* pages/voteWinner/voteWinner.wxss */
.winner{
    background-color: rgba(247,247,247,1);
}

.centerBox .centerTop {
  height:118rpx;
  border-radius:10rpx 10rpx 0 0;
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  z-index: 99;
}
.centerBox .title {
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  z-index: 5;
}
.centerBox .title>text{
    font-size:36rpx;
    font-weight:400;
    color: rgba(153,153,153,1);
    display: inline-block;
    height: 100%;
    line-height: 118rpx;
}
.centerBox .right {
  height:64rpx;
  background:#fff;
  border-radius:32rpx;
  display: flex;
  align-items: center;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  box-sizing: border-box;
  padding: 0 21rpx 0 24rpx;
}
.centerBox .right text {
  display: block;
  width: 32rpx;
  height: 32rpx;
  font-size: 22rpx;
  text-align: center;
  border: 2rpx solid rgba(102,102,102,1);
  border-radius: 50%;
  margin-right: 11rpx;
  color: rgba(102,102,102,1);
}

.centerBox .centerTop .one {
  width: 470rpx;
  height: 118rpx;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 1;
}
.centerBox .centerTop .three {
  width: 470rpx;
  height: 118rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.centerBox .centerTop .tow {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.centerBox .title .active {
    font-size:40rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(223,32,32,1);
    border-bottom: 4rpx solid #E13131;
}

.centerBox .center {
  position: fixed;
  top: 118rpx;
  left: 0;
  right: 0;
  height: 110rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  background-color: #F7F7F7;
  z-index: 99;
}
.centerBox .center text {
  color: rgba(102,102,102,1);
}
.centerBox .center .category{
  width:309rpx;
  height:72rpx;
  background:rgba(255,255,255,1);
  border-radius:10rpx;
  display: flex;
  align-items: center;
}
.centerBox .center .category .section__title,
.centerBox .center .category .picker
{
  height: 72rpx;
  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(50,50,50,1);
  line-height: 72rpx;
  text-align: center;
}
.centerBox .center .category .picker{ width: 150rpx; padding-left: 30rpx; text-align: left; }
.centerBox .center .category .picker input{ height: 100%; }
.centerBox .center .category .picker image{ width:24px; height:16px; }
.centerBox .center .category .picker .catename{
  position: relative;
}
.centerBox .center .category .picker .catename::after{
  content: '';
  position: absolute;
  top: 26rpx;
  right: 29rpx;
  border-right: 12rpx solid transparent;
  border-left: 12rpx solid transparent;
  border-top: 24rpx solid #CCCCCC;
  border-radius: 5rpx;
}
.centerBox .center .category .section__title{
  width: 128rpx;
  border-right: 1px solid #EDEDED;
}
.centerBox text {
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.showrank{
  margin: 228rpx 0 118rpx 0;
}
.share {
  width: 100%;
  height: 118rpx;
  background:rgba(223,32,32,1);
  position: fixed;
  bottom: 0;
  font-size:40rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
}

.toast {
  width:280rpx;
  height:177rpx;
  background:rgba(0,0,0,0.74);
  border-radius:10rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 446rpx;
  left: 50%;
  margin-left: -140rpx;
}
.toast image {
  width: 64rpx;
  height: 64rpx;
  margin-bottom: 7rpx;
}
.toast text {
  font-size:40rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
}

/* 个人奖项样式 */
.centerBox1 {
  margin: 0rpx 30rpx 0rpx 30rpx;
  background:rgba(255,255,255,1);
  /* border-radius: 0 0 10rpx 10rpx; */
  border-radius:10rpx;
  margin-bottom: 20rpx;
}

.centerBox1 .title {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:36rpx;
  font-weight:400;
  color: rgba(153,153,153,1);
  position: relative;
  z-index: 5;
}
.centerBox1 .content1 {
  padding: 0 30rpx;
  background:rgba(255,255,255,1);
} 
.centerBox1 .contentTop1 {
  height: 108rpx;
  border-radius:10rpx 10rpx 0 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  position: relative;
  background:rgba(255,255,255,1);
}
.centerBox1 .contentTop1 .tableTitle {
  width: 108rpx;
  text-align: center;
  line-height: 108rpx;
  font-size:36rpx;
  font-weight:600;
  color:rgba(51,51,51,1);
  font-family:PingFangSC-Medium,PingFang SC;
}
.centerBox1 .contentTop1 .tableTitle:nth-child(2) {
  width: 335rpx;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 0 10rpx;
}
.centerBox1 .contentTop1 .heng {
  height: 2rpx;
  background: rgba(237,237,237,1);
  position: absolute;
  bottom: 0;
  right: 30rpx;
  left: 30rpx;
}
.centerBox1 .content1 .list {
  height: calc(100vh - 454rpx);
}

.contentList {
  background: #fff;
  color:rgba(51,51,51,1);
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
  border-bottom: 1rpx solid rgba(237,237,237,1);
}
.contentList:last-child {
  border: 0;
}
 .contentList .number {
  width: 108rpx;
  font-size: 36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  height: 100%;
  text-align: center;
  margin-right: 30rpx;
}

.contentList .name{
  width: 335rpx;
  text-align: center;
  font-size:36rpx;
  font-weight:400;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 0 10rpx;
}
 .contentList .time {
  text-align: center;
  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-left: 30rpx;
}
.centerBox1 .content1 .contentList .img {
  width: 61rpx;
  height: 51rpx;
}