/* pages/voteWinner/voteWinner.wxss */
.winner{
    background-color: rgba(247,247,247,1);
}

.centerBox .centerTop {
  height:118rpx;
  border-radius:10rpx 10rpx 0 0;
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  z-index: 99;
}
.centerBox .title {
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  z-index: 5;
}
.centerBox .title>text{
    font-size:36rpx;
    font-weight:400;
    color: #999999;
    display: inline-block;
    height: 100%;
    line-height: 118rpx;
    position: relative;
}
.centerBox .right {
  height:64rpx;
  background:#fff;
  border-radius:32rpx;
  display: flex;
  align-items: center;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  box-sizing: border-box;
  padding: 0 21rpx 0 24rpx;
}
.centerBox .right text {
  display: block;
  width: 32rpx;
  height: 32rpx;
  font-size: 22rpx;
  text-align: center;
  border: 2rpx solid rgba(102,102,102,1);
  border-radius: 50%;
  margin-right: 11rpx;
  color: rgba(102,102,102,1);
}

.centerBox .centerTop .one {
  width: 470rpx;
  height: 118rpx;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 1;
}
.centerBox .centerTop .three {
  width: 470rpx;
  height: 118rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.centerBox .centerTop .tow {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.centerBox .title .active {
    font-size:36rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:#176AE6;
    border-bottom: 4rpx solid #176AE6;
}

.centerBox .center {
  position: fixed;
  top: 118rpx;
  left: 0;
  right: 0;
  height: 110rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  background-color: #F7F7F7;
  z-index: 99;
}
.centerBox .center text {
  color: rgba(102,102,102,1);
}
.centerBox .center .category{
  min-width:309rpx;
  height:72rpx;
  background:rgba(255,255,255,1);
  border-radius:10rpx;
  display: flex;
  align-items: center;
}
.centerBox .center .category .section__title,
.centerBox .center .category .picker .tag
{
  height: 72rpx;
  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(50,50,50,1);
  line-height: 72rpx;
  text-align: center;
}

.centerBox .center .category .picker .tag{
  display: flex;
  justify-content: space-between;
  align-items: center;
  text-align: center;
  position: relative;
}
.centerBox .center .category .picker .tag .catename{
  font-size: 36rpx;
  margin-left: 29rpx;
  max-width: 175rpx;
  overflow: hidden;  
  white-space: nowrap;
  text-overflow: ellipsis;
}
.centerBox .center .category .picker .tag .triangle{
  width: 0;
  height: 0;
  margin: 0 29rpx 0 26rpx;
  border-right: 16rpx solid transparent;
  border-left: 16rpx solid transparent;
  border-top: 22rpx solid #CCCCCC;
  border-radius: 5rpx;
}
.centerBox .center .category .section__title{
  width: 128rpx;
  border-right: 1px solid #EDEDED;
}
.centerBox text {
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.showrank{
  margin: 228rpx 0 118rpx 0;
  height: 100%;
}
.search-box{
  background: #fff;
  padding: 40rpx 30rpx 30rpx 30rpx;
}
.search-box .name{
  margin-bottom: 20rpx;
  font-size:40rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.search-box .ipt-name{
  width:690rpx;
  height:108rpx;
  line-height: 108rpx;
  background:rgba(247,247,247,1);
  border-radius:6rpx;
  margin-bottom: 40rpx;
  padding: 0 30rpx;
}
.search-footer{
  width: 100%;
  display: flex;
  justify-content: center;
}
.search-footer .search-btn{
  width:340rpx;
  height:88rpx;
  margin-top: 20rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius:6rpx;
  border:1px solid rgba(23,106,230,1);

  font-size:40rpx;
  font-family:PingFang-SC-Bold,PingFang-SC;
  font-weight:bold;
  color:rgba(23,106,230,1);
}
.ipt-palceholder{
  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(204,204,204,1);
}
.share {
  width: 100%;
  height: 118rpx;
  background:#176AE6;
  position: fixed;
  bottom: 0;
  font-size:40rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
}

.toast {
  width:280rpx;
  height:177rpx;
  background:rgba(0,0,0,0.74);
  border-radius:10rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 446rpx;
  left: 50%;
  margin-left: -140rpx;
}
.toast image {
  width: 64rpx;
  height: 64rpx;
  margin-bottom: 7rpx;
}
.toast text {
  font-size:40rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
}

/* 个人奖项样式 */
.centerBox1 {
  margin: 0rpx 30rpx 0rpx 30rpx;
  background:rgba(255,255,255,1);
  /* border-radius: 0 0 10rpx 10rpx; */
  border-radius:10rpx;
  margin-bottom: 20rpx;
}

.centerBox1 .title {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:36rpx;
  font-weight:400;
  color: rgba(153,153,153,1);
  position: relative;
  z-index: 5;
}
.centerBox1 .content1 {
  padding: 0 30rpx;
  background:rgba(255,255,255,1);
} 
.centerBox1 .contentTop1 {
  height: 108rpx;
  border-radius:10rpx 10rpx 0 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  position: relative;
  background:rgba(255,255,255,1);
}
.centerBox1 .contentTop1 .tableTitle {
  width: 108rpx;
  text-align: center;
  line-height: 108rpx;
  font-size:36rpx;
  font-weight:600;
  color:rgba(51,51,51,1);
  font-family:PingFangSC-Medium,PingFang SC;
}
.centerBox1 .contentTop1 .tableTitle:nth-child(2) {
  width: 335rpx;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 0 10rpx;
}
.centerBox1 .contentTop1 .heng {
  height: 2rpx;
  background: rgba(237,237,237,1);
  position: absolute;
  bottom: 0;
  right: 30rpx;
  left: 30rpx;
}
.centerBox1 .content1 .list {
  height: calc(100vh - 454rpx);
}

.contentList {
  background: #fff;
  color:rgba(51,51,51,1);
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
  border-bottom: 1rpx solid rgba(237,237,237,1);
}
.contentList:last-child {
  border: 0;
}
 .contentList .number {
  width: 108rpx;
  font-size: 36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  height: 100%;
  text-align: center;
  margin-right: 30rpx;
}

.contentList .name{
  width: 335rpx;
  text-align: center;
  font-size:36rpx;
  font-weight:400;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 0 10rpx;
}
 .contentList .time {
  text-align: center;
  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-left: 30rpx;
}
.centerBox1 .content1 .contentList .img {
  width: 61rpx;
  height: 51rpx;
}

/* canvas */
.certificate-can{
  width:630rpx;
  height:1000rpx;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: -630rpx;
  opacity: 0;
  z-index: -10;
}

.certificate-card{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background:rgba(0,0,0,0.6);
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  z-index: 99;
}
.card-content{
  width: 630rpx;
  height: 1000rpx;
  position: relative;
}
.card-content .card-bg{
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.card-author,
.card-opus,
.card-nick{
  font-size:36rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
}
.card-author{
  width: 255rpx;
  position: absolute;
  top: 325rpx;
  left: 86rpx;
  text-align: center;
  color: #333;
}
.card-opus{
  width: 120rpx;
  position: absolute;
  top: 418rpx;
  left: 230rpx;
  text-align: center;
  color: #333;
}
.card-nick{
  width: 190rpx;
  position: absolute;
  top: 598rpx;
  left: 318rpx;
  text-align: center;
  color:rgba(255,0,0,1);
}

.card-share{
  width: 630rpx;
  height: 108rpx;
  line-height: 108rpx;
  margin-top: 40rpx;
  display: flex;
  align-items: center;
}
.card-share .left,
.card-share .right{
  width: 50%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.card-share>.left>image,
.card-share>.right>image{
  width: 48rpx;
  height: 48rpx;
}
.card-share .left{
  background: #fff;
  border-radius:54rpx 0px 0px 54rpx;
  color:rgba(25,117,255,1);
}
.card-share>.left>.text,
.card-share>.right>.text{
  font-size:38rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  margin-left: 12rpx;
}
.card-share .right{
  color: #FFFFFF;
  background: #1975FF;
  border-radius:0px 54rpx 54rpx 0px;
}

.certificate-del{
  width: 72rpx;
  height: 72rpx;
  position: absolute;
  right: -30rpx;
  top: -30rpx;
}