/* pages/withdraw/withdraw.wxss */
page {
  background:rgba(244,244,244,1);
}
.container {
  min-height: 100vh;
}
.container .topBox {
  width: 730rpx;
  height: 378rpx;
  background:url('https://hwcdn.jinlingkeji.cn/images/pro/makeMoneytop.png') no-repeat center;
  background-size: 100% 100%;
  margin:16rpx auto 0 auto;
  position: relative;
}
.container .topBox .txt{
  display: block;
  font-size:38rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  float: left;
  margin-top: 46rpx;
  margin-left: 50rpx;
}
.container .topBox .withdrawRecord {
  display: flex;
  align-items: center;
  float: right;
  margin-right: 42rpx;
  margin-top: 54rpx;
}
.container .topBox .withdrawRecord text {
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.container .topBox .withdrawRecord image {
  width: 50rpx;
  height: 50rpx;
  margin-bottom: -9rpx;
}
.container .topBox .money {
  width: 100%;
  font-size:80rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(244,244,244,1);
  position: absolute;
  top: 129rpx;
  text-align: center;
}
.container .topBox .money text {
  font-size:28rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(244,244,244,1);
  margin-left: 10rpx;
}
.container .topBox .banner {
  width:630rpx;
  height:80rpx;
  position: absolute;
  top: 273rpx;
  left: 50%;
  margin-left: -315rpx;
  border-top: 1rpx solid rgba(255,255,255,0.4);
  display: flex;
  align-items: center;
}
.container .topBox .banner image {
  width: 38rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.container .topBox .banner .swiper {
  flex: 1;
  height: 100%;
  line-height: 80rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.container .centerBox {
  width:690rpx;
  min-height: calc(100vh - 616rpx);
  background:rgba(255,255,255,1);
  border-radius:10rpx;
  margin: 2rpx auto;
}
.container .centerBox .centerTop {
  height: 113rpx;
  border-bottom: 1rpx solid #E9E9E9;
  position: relative;
  padding-left: 30rpx;
  line-height: 113rpx;
  text-align: left;
  border-radius: 0;
}
.container .centerBox .centerTop text {
  font-size:38rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(58,58,58,1);
}
.container .centerBox .centerTop image {
  width:14rpx;
  height:24rpx;
  position: absolute;
  top: 50%;
  right: 30rpx;
  margin-top: -12rpx;
}
.container .centerBox .windrawlist {
  padding:30rpx;
}
.container .centerBox .windrawlist text {
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(126,126,126,1);
}
.container .centerBox .windrawlist .listBox {
  margin-top: 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.container .centerBox .windrawlist .listBox button {
  width:190rpx;
  height:90rpx;
  line-height: 90rpx;
  border-radius:6rpx;
  border:1px solid rgba(254,155,2,1);
  font-size:40rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(254,155,2,1);
  margin-bottom: 30rpx;
  margin-right: 30rpx;
}
.container .centerBox .windrawlist .listBox button:nth-child(3n) {
  margin-right: 0;
}
.container .kong {
  height: 280rpx;
  color: rgba(244,244,244,1);
  opacity: 0;
}
.toast  {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.6);
  z-index: 50;
}
.toastBox {
  width:580rpx;
  height:482rpx;
  background:rgba(255,255,255,1);
  border-radius:20rpx;
  position: relative;
  top: 284rpx;
  left: 50%;
  margin-left: -290rpx;
}
.toastBox image:nth-child(1) {
  width: 146rpx;
  height: 146rpx;
  position: absolute;
  top: -73rpx;
  left: 50%;
  margin-left: -73rpx;
}
.toastBox text:nth-child(2) {
  display: block;
  width: 100;
  font-size:36rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(126,126,126,1);
  text-align: center;
  position: relative;
  top: 100rpx;
}
.toast .number {
  width: 100%;
  text-align: center;
  margin-top: 138rpx;
  font-size:80rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(58,58,58,1);
}
.toast .number text {
  display: inline-block;
  font-size:28rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(58,58,58,1);
  margin-left: -14rpx;
}
.toast .button {
  width:480rpx;
  height:90rpx;
  line-height: 90rpx;
  background:rgba(254,155,2,1);
  border-radius:45px;
  position: absolute;
  bottom: 48rpx;
  left: 50%;
  margin-left: -240rpx;
  font-size:36rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  text-align: center;
}
.toast .cancel {
  position: absolute;
  bottom: -131rpx;
  left: 50%;
  margin-left: -44rpx;
}
