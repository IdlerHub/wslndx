<!--pages/withdrawalRecord/withdrawalRecord.wxml-->
<view class="container" wx:if="{{list.length > 0}}">
  <view class="item" wx:for="{{list}}" wx:key="index">
    <view class="left">
      <text>{{item.amount}}元</text>
      <text>{{item.createtime}}</text>
    </view>
    <view class="{{item.status != '提现中'  ? 'right' : 'rightnow'}}">{{item.status != '提现中' ? '已到账' : '提现中'}}</view>
  </view>
</view>
<view class="qst" wx:else>
  <image src="{{$state.imgHost}}/myApprenticeqst.png"></image>
  <text>暂无记录，去邀请好友</text>
</view>