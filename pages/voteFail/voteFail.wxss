/* pages/voteFail/voteFail.wxss */
.none-production{
    display: flex;
    justify-content: space-around;
    flex-flow: column;
    align-items: center;
}
.none-production .none-img{
    width: 316rpx;
    height: 196rpx;
    margin-top: 119rpx;
    margin-bottom: 54rpx;
}
image{
    width: 100%;
    height: 100%;
}
.fail-content{
    display: flex;
    justify-content: space-around;
    flex-flow: column;
    align-items: center;
}
.fail-content text{
    height:56rpx;
    font-size:40rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(51,51,51,1);
    line-height:56rpx;
    margin-bottom: 11rpx;
}
.fail-content view{
    width: 607rpx;
    height:100rpx;
    font-size:36rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(102,102,102,1);
    line-height:50rpx;
    text-align: center;
    margin-bottom: 22rpx;
}
.fail-btn{
    width:420rpx;
    height:88rpx;
    background:#1975FF;
    border-radius:6rpx;
    text-align: center;
    line-height: 88rpx;

    font-size:40rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(255,255,255,1);
}