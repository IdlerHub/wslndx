/* pages/upwxpage/upwxpage.wxss */

.container {
  text-align: center;
}

.container .content {
  width: 100%;
  background: #fff;
  margin-top: 122rpx;
}

.content .logo {
  width: 160rpx;
  height: 160rpx;
  margin-bottom: 30rpx;
}

.content .logo-title {
  display: block;
  font-size: 44rpx;
  color: #3a3a3a;
  letter-spacing: 1rpx;
}

.content .logo-content {
  width: 598rpx;
  height: 112rpx;
  font-size: 36rpx;
  font-weight: 400;
  color: rgba(126, 126, 126, 1);
  line-height: 56rpx;
  margin: 30rpx auto;
}

.content .upwx {
  width: 650rpx;
  height: 100rpx;
  background: rgba(75, 168, 45, 1);
  border-radius: 6rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 100rpx;
  text-align: center;
  margin: 174rpx auto 30rpx auto;
}

.content .cancel {
  width: 650rpx;
  height: 100rpx;
  border-radius: 6rpx;
  border: 1px solid rgba(126, 126, 126, 1);
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(126, 126, 126, 1);
  line-height: 100rpx;
  margin: 0 auto;
}
