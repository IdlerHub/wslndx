page .container > .top {
  position: relative;
  height: 260rpx;
  overflow: hidden;
  border-bottom: 20rpx solid #f4f4f4;
}

page .container > .top .bg {
  position: absolute;
  left: -25rpx;
  bottom: 0;
  top: 0;
  margin: auto;
  width: 800rpx;
  filter: blur(20rpx) brightness(0.65);
  transform: scale(1.2);
}

page .container > .top .box {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 260rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}

page .container > .top .box .left {
  display: flex;
  align-items: center;
}

page .container > .top .box .left .img {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  border: 2rpx solid #b1adac;
}

page .container > .top .box .left .info {
  margin: 0 20rpx;
}

page .container > .top .box .left .info .line {
  text-align: left;
  line-height: 62rpx;
}

page .container > .top .box .left .info .name {
  width: 440rpx;
  font-family: PingFangSC-Medium;
  font-size: 44rpx;
  line-height: 62rpx;
  color: #ffffff;
}

page .container > .top .box .left .info .city {
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #ffffff;
  line-height: 40rpx;
  border-radius: 4rpx;
  background: #b1adac;
  padding: 0 15rpx;
}

page .container > .top .box .left .info .school {
  margin-left: 15rpx;
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #ffffff;
  line-height: 40rpx;
  border-radius: 4rpx;
  background: #b1adac;
  padding: 0 15rpx;
}

page .container > .top .box .iconfont {
  color: white;
}

page .list {
  padding: 0 30rpx;
}

