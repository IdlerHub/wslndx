page .container > .top {
  position: relative;
  height: 300rpx;
  overflow: hidden;
}

page .container > .top .bg {
  position: absolute;
  left: -25rpx;
  bottom: 0;
  top: 0;
  margin: auto;
  width: 800rpx;
  filter: blur(20rpx) brightness(0.65);
  transform: scale(1.2);
}

page .container > .top .box {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 260rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}

page .container > .top .box .left {
  display: flex;
  align-items: center;
}

page .container > .top .box .left .img {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  border: 2rpx solid #b1adac;
}

page .container > .top .box .left .info {
  margin: 0 20rpx;
}

page .container > .top .box .left .info .line {
  text-align: left;
  line-height: 62rpx;
}

page .container > .top .box .left .info .name {
  width: 440rpx;
  font-family: PingFangSC-Medium;
  font-size: 44rpx;
  line-height: 62rpx;
  color: #ffffff;
}

page .container > .top .box .left .info .city {
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #ffffff;
  line-height: 40rpx;
  border-radius: 4rpx;
  background: #999594;
  padding: 0 15rpx;
}

page .container > .top .box .left .info .school {
  margin-left: 15rpx;
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #ffffff;
  line-height: 40rpx;
  border-radius: 4rpx;
  background: #999594;
  padding: 0 15rpx;
}

page .container > .top .box .iconfont {
  color: white;
}

page .list {
  background: white;
  position: relative;
  margin-top: -40rpx;
  border-top-left-radius: 40rpx;
  border-top-right-radius: 40rpx;
  padding: 0 30rpx;
  overflow: hidden;
}

page .list .score {
  margin-top: 60rpx;
}

page .list .score .title {
  font-size: 36rpx;
  font-weight: 400;
  color: #ff1b1b;
  line-height: 50rpx;
}

page .list .score .content {
  margin-left: -20rpx;
  width: 730rpx;
  height: 200rpx;
  position: relative;
}

page .list .score .content .contain {
  position: absolute;
  left: 50rpx;
  top: 50rpx;
  height: 100rpx;
}

page .list .score .content .contain .num {
  font-size: 60rpx;
  font-weight: 600;
  color: white;
  line-height: 100rpx;
}

page .list .score .content .contain .dsc {
  margin-left: 10rpx;
  font-size: 24rpx;
  color: white;
  line-height: 33rpx;
}

page .list .score .content .score-btn {
  position: absolute;
  right: 50rpx;
  top: 70rpx;
  width: 160rpx;
  height: 60rpx;
  background: white;
  border-radius: 30rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #ff1a1a;
  line-height: 60rpx;
  text-align: center;
}

