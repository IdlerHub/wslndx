page {
  background: #f7f7f7;
  height: 100%;
}
page .container {
  height: calc(100% + 120rpx);
}
page .container > .top {
  position: relative;
  height: 240rpx;
  overflow: hidden;
}

page .container > .top .bg {
  position: absolute;
  left: -25rpx;
  bottom: 0;
  top: 0;
  margin: auto;
  width: 800rpx;
  filter: blur(20rpx) brightness(0.65);
  transform: scale(1.2);
}

page .container > .top .box {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 300rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}

page .container > .top .box .left {
  display: flex;
  align-items: center;
  margin-top: -70rpx;
}

page .container > .top .box .left .img {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  border: 2rpx solid #b1adac;
}

page .container > .top .box .left .info {
  margin: 0 20rpx;
}

page .container > .top .box .left .info .line {
  text-align: left;
  line-height: 62rpx;
}

page .container > .top .box .left .info .name {
  width: 440rpx;
  font-family: PingFangSC-Medium;
  font-size: 44rpx;
  line-height: 62rpx;
  color: #fff;
}

page .container > .top .box .left .info .city {
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #fff;
  line-height: 40rpx;
  border-radius: 4rpx;
  background: #999594;
  padding: 0 15rpx;
}

page .container > .top .box .left .info .school {
  margin-left: 20rpx;
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #fff;
  line-height: 40rpx;
  border-radius: 4rpx;
  background: #999594;
  padding: 0 15rpx;
}

page .container > .top .box .iconfont {
  color: white;
  margin-top: -70rpx;
}

page .scoreBg {
  display: block;
  width: 730rpx;
  height: 220rpx;
  position: absolute;
  z-index: -1;
  left: 12rpx;
}

page .navgationto {
  width: 100%;
  height: 240rpx;
  padding: 14rpx 10rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: rowl;
  justify-content: space-around;
}

page .navgationto .navgationBox {
  position: relative;
}

page .navgationto .navgationBox image {
  width: 366rpx;
  height: 220rpx;
}

page .navgationto .navgationBox .navTitle {
  font-size: 42rpx;
  font-weight: 600;
  color: rgba(255, 255, 255, 1);
  position: absolute;
  top: 55rpx;
  left: 56rpx;
  letter-spacing: -1rpx;
}

page .navgationto .navgationBox .num {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 0.9);
  position: absolute;
  left: 56rpx;
  bottom: 56rpx;
}

page .navgationto .navgationBox .toScore {
  width: 44rpx;
  height: 44rpx;
  background: rgba(255, 123, 124, 1);
  box-shadow: 0px 4px 8px 0px rgba(255, 0, 36, 1);
  border-radius: 50%;
  position: absolute;
  top: 65rpx;
  right: 92rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

page .navgationto .navgationBox .toScore image {
  height: 23rpx;
  width: 24rpx;
}

page .navgationto .navgationBox .toInvite {
  width: 44rpx;
  height: 44rpx;
